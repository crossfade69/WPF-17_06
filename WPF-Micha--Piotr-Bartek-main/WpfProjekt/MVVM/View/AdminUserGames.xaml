<Window x:Class="WpfProjekt.MVVM.View.AdminUserGames"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfProjekt.MVVM.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminUserGames">

    <Grid>
        <Grid Background="#FF2D2D2D">
            <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,5,40" SelectionChanged="UserGamesList_SelectionChanged"/>
            

                
                <Button Name="DeleteGameButton" Margin="679,399,5,5" Grid.Row="0" Content="Usuń" Click="DeleteGameButton_Click"/>
        </Grid>
    </Grid>
</Window>
